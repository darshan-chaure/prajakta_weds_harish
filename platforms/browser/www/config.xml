<?xml version='1.0' encoding='utf-8'?>
<widget id="com.darshanchaure.prajakta_weds_harish" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Prajakta Weds Harish</name>
    <description>
        Wedding invitation app to invite guests in a wedding ceremony of Prajakta and Harish.
    </description>
    <author email="cdarshan1990@gmail.com">
        Darshan Chaure
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="icon.png" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <preference name="orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-targetSdkVersion" value="26" />
</widget>
